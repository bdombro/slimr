{
  "name": "@slimr/react",
  "version": "3.0.19",
  "author": "Brian Dombrowski",
  "license": "ISC",
  "private": false,
  "sideEffects": false,
  "keywords": [
    "preact",
    "react",
    "swr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bdombro/slimr.git"
  },
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:esm": "OUT=esm; tsc -d --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "build:cjs": "OUT=cjs; tsc -d -m commonjs --moduleResolution node --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "clean": "rm -rf esm cjs",
    "prepack": "run-s build",
    "start": "nodemon -w src -e '*' -x 'npm run build && cd ../demo && npm start'"
  },
  "dependencies": {
    "@slimr/forms": "^5.0.6",
    "@slimr/markdown": "^2.1.42",
    "@slimr/router": "^2.1.55",
    "@slimr/styled": "^2.1.49",
    "@slimr/swr": "^2.1.37",
    "@slimr/util": "^3.2.40",
    "react-use": "17"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "*",
    "react-dom": "*"
  }
}
